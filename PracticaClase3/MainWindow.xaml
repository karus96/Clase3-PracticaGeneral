<Window x:Class="PracticaClase3.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PracticaClase3"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <Label>Nombre</Label>
        <TextBox Name="txtNombre"></TextBox>
        <Label>Tipo de energia</Label>
        <TextBox Name="txtTipoEnergia"></TextBox>
        <Label>Color</Label>
        <TextBox Name="txtColor"></TextBox>
        <Label>Marca</Label>
        <TextBox Name="txtMarca"></TextBox>
        <Button Click="Button_Click">Crear vehículo</Button>
        <TextBlock Name="Data">No data</TextBlock>
    </StackPanel>
</Window>
